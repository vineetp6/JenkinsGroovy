node {
    stage('SCM') {
	echo 'Gathering code from version control'
	git branch: '${branch}', url: 'https://github.com/vineetp6/JenkinsGroovy.git'
    }
    stage('Build') {
       try{
        echo 'Building....'
	sh 'dotnet --version'
	sh 'dotnet build ConsoleApp1'
	echo 'new feature'
	releasenotes()
	} catch(ex){
	   echo "something Wrong"
	 }
	 finally{
	 }
    }
    stage('Test') {
        echo 'Testing....'
    }
    stage('Deploy') {
        echo 'Deploying....'
    }
}
